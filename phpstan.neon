parameters:
    level: 9
    paths:
        - src
    excludePaths:
    		- src/DataFixtures/*
    reportUnmatchedIgnoredErrors: false
    checkGenericClassInNonGenericObjectType: false
    ignoreErrors:
    - '#Method [a-zA-Z0-9\\_]+::[a-zA-Z0-9]*[()]* has no return type specified#'
    symfony:
        containerXmlPath: var/cache/dev/App_KernelDevDebugContainer.xml